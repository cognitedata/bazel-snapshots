{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      matchUpdateTypes: [
        'patch',
        'pin',
        'digest',
      ],
      automerge: true,
    },
    {
      groupName: 'gazelle',
      matchPackageNames: [
        '/gazelle/',
      ],
    },
    {
      groupName: 'rules_go',
      matchPackageNames: [
        '/rules_go/',
      ],
    },
  ],
}
