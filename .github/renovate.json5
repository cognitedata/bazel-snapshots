{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    },
    {
      matchPackagePatterns: ["gazelle"],
      groupName: "gazelle",
    },
    {
      matchPackagePatterns: ["rules_go"],
      groupName: "rules_go",
    },
  ]
}
